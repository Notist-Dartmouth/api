{
  "name": "notist",
  "version": "1.0.0",
  "description": "React with Redux, React Router, Redial, Aphrodite, Express, Webpack.",
  "main": "server/app.js",
  "scripts": {
    "test": "mocha --require babel-core/register",
    "dev": "nodemon server/app.js --exec babel-node",
    "coverage": "nyc npm test",
    "start": "babel-node server/app.js",
    "prod": "npm run build; node dist/server.js",
    "start:old": "cross-env NODE_ENV=development node -r \"babel-register\" ./server",
    "start:byrneprod": "cross-env NODE_ENV=production node ./build/server.js",
    "heroku-postbuild": "npm run build",
    "build": "babel app -d dist",
    "build:byrne": "webpack -p --config ./tools/webpack.client.prod.js && webpack -p --config ./tools/webpack.server.prod.js",
    "clean:byrne": "rm -rf build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/notist/notist.git"
  },
  "keywords": [],
  "author": "Byrne Hollander",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/notist"
  },
  "homepage": "https://github.com/notist",
  "dependencies": {
    "aphrodite": "^1.1.0",
    "assets-webpack-plugin": "^3.3.0",
    "axios": "^0.15.0",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "^6.9.0",
    "body-parser": "^1.14.2",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "connect-mongo": "^1.3.2",
    "cors": "^2.7.1",
    "cross-env": "2.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.13.4",
    "express-session": "^1.15.0",
    "helmet": "^2.0.0",
    "history": "^4.0.0",
    "hpp": "^0.2.0",
    "json-loader": "^0.5.4",
    "material-ui": "^0.17.1",
    "mocha": "^3.2.0",
    "mongodb": "^2.2.24",
    "mongoose": "^4.7.8",
    "morgan": "^1.6.1",
    "normalize-url": "^1.9.0",
    "opn": "^4.0.2",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth2": "^0.1.6",
    "passport-stub": "^1.1.1",
    "path": "^0.12.7",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-helmet": "^3.1.0",
    "react-icons": "^2.2.3",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "redial": "^0.4.1",
    "redux": "^3.3.1",
    "redux-thunk": "^2.1.0",
    "serialize-error": "^2.1.0",
    "source-map-support": "^0.4.0",
    "standard": "^8.0.0",
    "standard-loader": "^5.0.0",
    "throng": "^4.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },

  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "mocha": "^3.0.2",
    "nodemon": "^1.10.0",
    "nyc": "^10.0.0"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "engines": {
    "node": "6.2.2"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "**/migrations/**",
      "**/tools/**",
      "**/test/**"
    ]
  }
}
